public class Main {

    public static void main(String[] args) {
        int x = Integer.parseInt(args[0]);
        int y = Integer.parseInt(args[1]);
        System.out.println(task6(x, y));
}

    public static boolean task6(int x, int y) {
        if (y > -4 && y < 5) {
            return (y < 1) ? (x > -5 && x < 5):(x > -3 && x < 3);
        }
        return false;
    }
}
