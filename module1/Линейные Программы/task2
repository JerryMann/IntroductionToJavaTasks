public class Main {

    //пока без валидаций
    public static void main(String[] args) {
        double a = Double.parseDouble(args[0]);
        double b = Double.parseDouble(args[1]);
        double c = Double.parseDouble(args[2]);
        
        //task2 вычислите значение выражения по формуле
        double result2 = ((b + Math.sqrt(b*b + 4 * a * c))/(2 * a)) - (Math.pow(a,3) * c) + Math.pow(b,-2);
    }
}
